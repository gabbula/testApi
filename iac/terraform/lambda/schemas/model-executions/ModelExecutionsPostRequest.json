{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ModelExecutionsPostRequest",
  "type": "object",
  "properties": {
    "model": {
      "type": "string",
      "description": "The name of the model to run."
    },
    "modelVersion": {
      "type": "string",
      "description": "The version of the model."
    },
    "callbackUrl": {
      "type": "string",
      "description": "An optional webhook URL to call upon completion or failure."
    }
  },
  "required": [
    "model",
    "modelVersion"
  ]
}
